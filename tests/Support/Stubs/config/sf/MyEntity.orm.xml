<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Somnambulist\Domain\Tests\Support\Stubs\Models\MyEntity">
        <id name="id" type="uuid">
            <generator strategy="NONE"/>
        </id>

        <field name="name" type="string"/>
        <field name="another" type="string"/>
        <field name="createdAt" type="datetime"/>
        <field name="updatedAt" type="datetime"/>

        <one-to-many field="related" target-entity="Somnambulist\Domain\Tests\Support\Stubs\Models\MyOtherEntity" mapped-by="myEntity">
            <cascade>
                <cascade-persist />
                <cascade-remove />
            </cascade>
        </one-to-many>
    </entity>
</doctrine-mapping>
