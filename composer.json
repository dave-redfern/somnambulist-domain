{
    "name": "somnambulist/domain",
    "type": "library",
    "description": "Provides a collection of entities, helpers and base implementations for creating domain objects.",
    "keywords": [
        "value objects",
        "entities",
        "ddd",
        "domain events",
        "enumerations",
        "aggregate root"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Redfern",
            "email": "info@somnambulist.tech"
        }
    ],
    "autoload": {
        "psr-4": {
            "Somnambulist\\Domain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Somnambulist\\Domain\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/Doctrine/_data/",
            "tests/Events/_data/"
        ],
        "files": [
            "tests/Events/_data/helpers.php"
        ]
    },
    "require": {
        "php": ">=7.2",
        "ext-bcmath": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "beberlei/assert": "~3.0",
        "eloquent/enumeration": "~5.1|~6.0",
        "pragmarx/ia-str": "~5.8",
        "somnambulist/collection": "~3.0"
    },
    "require-dev": {
        "doctrine/orm": "~2.6",
        "illuminate/events": "~5.4",
        "myclabs/php-enum": "~1.7",
        "nesbot/carbon": "~2.0",
        "pagerfanta/pagerfanta": "~2.1",
        "phpunit/phpunit": "~8.2",
        "ramsey/uuid": "~3.7",
        "symfony/contracts": "~1.1",
        "symfony/event-dispatcher": "~4.0",
        "symfony/messenger": "~4.0",
        "symfony/property-access": "~4.0",
        "symfony/serializer": "~4.0",
        "symfony/var-dumper": "~4.0",
        "symfony/yaml": "4.0"
    },
    "suggest": {
        "giggsey/libphonenumber-for-php": "To validate phone numbers / convert to E164 from a local format",
        "propaganistas/laravel-phone": "A Laravel mapping for the Giggsey",
        "doctrine/orm": "To use the Doctrine components and have an entity mapper",
        "laravel-doctrine/orm": "A Laravel mapping for Doctrine",
        "pagerfanta/pagerfanta": "For Pagination of Doctrine results and other data",
        "symfony/messenger": "(A)Sync Bus implementation for Symfony needed for query/command/event bus messenger implementations"
    }
}
